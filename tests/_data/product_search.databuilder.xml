<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="LocalizedAttributes">
        <property name="isSearchable" dataBuilderRule="=1"/>
    </transfer>

    <transfer name="ProductAttributeKey">
        <property name="key" dataBuilderRule="uuid()" type="string"/>
    </transfer>

    <transfer name="ProductSearchAttribute">
        <property name="position" dataBuilderRule="randomNumber(5)" type="int"/>
        <property name="filterType" dataBuilderRule="=multi-select" type="string"/>
    </transfer>

</transfers>
